/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}


button::after {
  border: none;
  border-radius: 0;
}

.paper-like {
  background-image: url('data:image/png;base64,');
}

.main-btn {
  background: url('data:image/png;base64,'), #a42d2d;
  width: 41.067vw;
  height: 12.267vw;
  font-family: LiDeBiao-Xing-3.0;
  font-size: 4.879vw;
  letter-spacing: 0.973vw;
  color: #f4f4f4;
  border-radius: 0;
  box-shadow: 0vw 0.8vw 0.8vw 0vw rgba(70, 6, 6, 0.4);
  animation: 1s show_scale ease-out backwards;
}

.main-btn.line {
  background: linear-gradient(90deg, #a42d2d 5%, #fff 5%, #fff 6%, #a42d2d 6%, #a42d2d 94%, #fff 94%, #fff 95%, #a42d2d 95%);
}

.btn-next {
  width: 38.776vw;
  height: 8.163vw;
  margin-top: 4.971vh;
  background-color: #3f1921;
  font-family: SourceHanSansCN-Normal;
  font-weight: normal;
  font-stretch: normal;
  font-size: 3.061vw;
  letter-spacing: 0vw;
  color: #dcc8ac;
  outline: 1.02vw solid #f5d8b3;
  border-radius: 0;
  box-shadow: 0.143vw 0.143vw 0.51vw 0vw #26180c inset, 0.143vw 0.143vw 0.51vw 0vw #26180c;
}

@keyframes fadeinT {
  0% {
    opacity: 0;
    transform: translateY(-100px);
  }

  100% {
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes show_scale {
  0% {
    opacity: 0;
    transform: scale(0);
  }

  100% {
    transform: scale(1);
    opacity: 1;
  }
}